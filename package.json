{
  "name": "good-samaritan",
  "description": "Find open issues from open source dependencies of your project.",
  "version": "0.0.0",
  "author": "Scott Davis @stdavis",
  "bin": {
    "good-samaritan": "./bin/run"
  },
  "bugs": "https://github.com/stdavis/good-samaritan/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@octokit/oauth-login-url": "2.1.2",
    "@octokit/rest": "16.31.0",
    "cli-ux": "5.3.3",
    "package-json": "6.5.0",
    "parse-github-url": "1.0.2",
    "read-pkg": "5.2.0",
    "stoppable": "1.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-agrc": "1.3.0",
    "eslint-plugin-html": "6.0.0",
    "mocha": "^5",
    "npm-watch": "0.6.0",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/stdavis/good-samaritan",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "good-samaritan"
  },
  "repository": "stdavis/good-samaritan",
  "scripts": {
    "testrun": "./bin/run",
    "posttest": "eslint .",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "watch": "npm-watch"
  },
  "watch": {
    "testrun": "src/*.js"
  }
}
